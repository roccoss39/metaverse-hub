# ✅ BEZPIECZNY przykład konfiguracji
# Skopiuj do .env.local i uzupełnij prawdziwymi wartościami

# Przelewy24 Configuration
P24_MERCHANT_ID=your_merchant_id
P24_POS_ID=your_pos_id
P24_API_KEY=your_secret_api_key
P24_CRC=your_secret_crc
P24_TEST_MODE=true

# Application URLs
NEXT_PUBLIC_APP_URL=http://localhost:3000

# PayPal Configuration (SANDBOX)
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your_paypal_sandbox_client_id
PAYPAL_CLIENT_SECRET=your_paypal_sandbox_client_secret

# ⚠️ NIGDY nie commituj .env.local do git!